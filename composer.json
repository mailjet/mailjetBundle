    "type": "symfony-bundle",
    "homepage": "https://github.com/mailjet/mailjetBundle",
    "license": "MIT",
    "keywords": [
        "Symfony",
        "MailJet",
        "API",
        "V3",
        "User",
        "bundle"
    ],
    "authors": [
        {
            "name": "Titouan BENOIT",
            "email": "titouan.benoit@gmx.fr",
            "homepage": "https://github.com/Nightbr"
        },
        {
            "name": "Mailjet API",
            "email": "api@mailjet.com",
            "homepage": "https://dev.mailjet.com/"
        }
