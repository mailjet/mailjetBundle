    "autoload": {
        "psr-4": {
            "Mailjet\\MailjetBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6",
        "mailjet/mailjet-apiv3-php": "^1.2",
        "mailjet/mailjet-swiftmailer": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2@dev",
